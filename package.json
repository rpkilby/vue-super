{
  "name": "vue-super",
  "version": "0.0.2",
  "description": "Vue.js plugin that allows you to reference methods on parent classes",
  "main": "vue-super.js",
  "scripts": {
    "test": "npm run lint && tap test.js --cov --nyc-arg=--require=babel-register",
    "report-coverage": "nyc report --reporter=lcov && codecov",
    "lint": "eslint vue-super.js test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpkilby/vue-super.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "super",
    "inheritance"
  ],
  "author": "Ryan P Kilby <rpkilby@ncsu.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpkilby/vue-super/issues"
  },
  "homepage": "https://github.com/rpkilby/vue-super#readme",
  "jspm": {
    "format": "esm"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "codecov": "^1.0.1",
    "eslint": "^3.17.1",
    "eslint-config-itng": "github:itng/eslint-config-itng#0.0.1",
    "tap": "^10.3.0",
    "vue": "*"
  }
}
